        android:textColor="#FFFFFF"
        android:textSize="22sp"
        app:layout_constraintBottom_toTopOf="@+id/btCos"
        app:layout_constraintEnd_toStartOf="@+id/btInvTan"
        app:layout_constraintHorizontal_bias="0.5"
